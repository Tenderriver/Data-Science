   "source": [
    "x = int (input (\"Nhap vao so nguyen n: \"))\n",
    "a=0\n",
    "b=0\n",
    "c=1\n",
    "for i in range (1, x+1):\n",
    "    if i%2 !=0:\n",
    "        a+=i\n",
    "    else:\n",
    "        b+=i\n",
    "    c*=i\n",
    "print (\"a=\",a , \"\\nb=\", b, \"\\nc=\", c)\n",
    "\n",
    "        "
